version: '3.5'


services:
  nginx:
    env_file: .env
    container_name: nginx
    image: nginx:alpine

    ports:
      # - "80:80"
      - "443:443"
    volumes:
      - ./root:/usr/local/nginx/html
      - ./conf.d:/etc/nginx/conf.d
      - ./log:/var/log/nginx/
      - /etc/cert:/etc/ssl/certs



networks:
  default:
    external:
      name: scan-network